{
	"compilerOptions": {
		"target": "es2020",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"strict": true
	},
	"include": ["parser/**/*.ts"]
}
